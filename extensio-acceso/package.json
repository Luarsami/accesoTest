    "jest-environment-jsdom": "^30.0.0",
    "jest-expo": "~53.0.7",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.3",
    "react-native-gesture-handler": "~2.24.0",
